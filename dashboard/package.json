{
  "name": "dashboard",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:review": "eval \"echo \\\"$(cat src/environments/environment.review.ts.template)\\\"\" | sed 's|$REVIEW_ID|'$REVIEW_ID'|g' | sed 's|$BRANCH|'$BRANCH'|g' > src/environments/environment.review.ts && cp -f src/environments/environment.review.ts src/environments/environment.ts && ng build --configuration=review",
    "build": "ng build --configuration=$NODE_ENV",
    "lint": "eslint 'src/**/*.ts'",
    "test:integration:debug": "yarn run cypress run --env ENV_NAME=local --spec 'cypress/integration/*.integration.ts'",
    "test:integration": "yarn run cypress run --spec cypress/integration/*.integration.ts",
    "open:cypress:debug": "yarn run cypress open --env ENV_NAME=local",
    "open:cypress": "yarn run cypress open"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.1.1",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "^8.0.2",
    "@angular/material-moment-adapter": "^8.1.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ctrl/ngx-chartjs": "^2.0.0",
    "@sentry/browser": "^5.24.2",
    "@types/leaflet": "^1.5.1",
    "@types/leaflet.markercluster": "^1.4.0",
    "chart.js": "^2.8.0",
    "hammerjs": "^2.0.8",
    "leaflet": "^1.5.1",
    "leaflet.markercluster": "^1.4.1",
    "lodash": "^4.17.15",
    "mathjs": "^6.1.0",
    "moment": "^2.24.0",
    "ng2-nouislider": "^1.8.2",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-toastr": "^10.0.4",
    "nouislider": "^14.0.2",
    "ol": "^5.3.3",
    "rxjs": "~6.6.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.29",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@types/node": "12.12.11",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "codelyzer": "^5.0.0",
    "cypress": "^3.4.1",
    "husky": "^3.0.9",
    "ts-loader": "^6.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.1",
    "typescript": "~3.5.3"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}