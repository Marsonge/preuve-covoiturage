{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://api.covoiturage.beta.gouv.fr/services/sample/user.schema.json",
  "type": "object",
  "title": "User",
  "required": [
    "_id",
    "email",
    "password",
    "lastname",
    "firstname",
    "phone",
    "group",
    "role",
    "permissions",
    "status",
    "hasResetPassword",
    "forgottenAt",
    "lastConnectedAt",
    "updatedAt",
    "createdAt",
    "fullname"
  ],
  "properties": {
    "_id": {
      "$id": "#/properties/_id",
      "type": "string",
      "title": "The _id Schema",
      "default": "",
      "pattern": "^(.*)$",
      "maxLength": 24,
      "minLength": 24
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "format": "email",
      "pattern": "^(.*)$",
      "maxLength": 256,
      "minLength": 5
    },
    "password": {
      "$id": "#/properties/password",
      "type": "string",
      "title": "The Password Schema",
      "default": "",
      "format": "password",
      "pattern": "^(.*)$",
      "maxLength": 256,
      "minLength": 6
    },
    "lastname": {
      "$id": "#/properties/lastname",
      "type": "string",
      "title": "The Lastname Schema",
      "default": "",
      "pattern": "^(.*)$",
      "maxLength": 256,
      "minLength": 1
    },
    "firstname": {
      "$id": "#/properties/firstname",
      "type": "string",
      "title": "The Firstname Schema",
      "default": "",
      "pattern": "^(.*)$",
      "maxLength": 256,
      "minLength": 1
    },
    "phone": {
      "$id": "#/properties/phone",
      "type": "string",
      "title": "The Phone Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "group": {
      "$id": "#/properties/group",
      "type": "string",
      "title": "The Group Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "role": {
      "$id": "#/properties/role",
      "type": "string",
      "title": "The Role Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "permissions": {
      "$id": "#/properties/permissions",
      "type": "array",
      "title": "The Permissions Schema",
      "items": {
        "$id": "#/properties/permissions/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "pattern": "^(.*)$"
      }
    },
    "status": {
      "$id": "#/properties/status",
      "type": "string",
      "title": "The Status Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "hasResetPassword": {
      "$id": "#/properties/hasResetPassword",
      "type": "boolean",
      "title": "The Hasresetpassword Schema",
      "default": false
    },
    "forgottenAt": {
      "$id": "#/properties/forgottenAt",
      "type": "string",
      "title": "The Forgottenat Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "lastConnectedAt": {
      "$id": "#/properties/lastConnectedAt",
      "type": "string",
      "title": "The Lastconnectedat Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "updatedAt": {
      "$id": "#/properties/updatedAt",
      "type": "string",
      "title": "The Updatedat Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "string",
      "title": "The Createdat Schema",
      "default": "",
      "pattern": "^(.*)$"
    },
    "fullname": {
      "$id": "#/properties/fullname",
      "type": "string",
      "title": "The Fullname Schema",
      "default": "",
      "pattern": "^(.*)$"
    }
  }
}
