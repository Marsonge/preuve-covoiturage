{
  "name": "@pdc/proxy",
  "version": "0.0.1",
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "yarn build && yarn watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\""
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "config": {
    "workingDir": "./dist"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/helmet": "^0.0.43",
    "@types/node": "^11.13.7",
    "@types/passport": "^1.0.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.11",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.15.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-import-group-ordering": "^2.1.1",
    "typescript": "^3.4.3"
  },
  "dependencies": {
    "@pdc/core": "0.0.1",
    "@pdc/provider-sentry": "0.0.1",
    "@pdc/service-notification": "0.0.1",
    "body-parser": "^1.19.0",
    "bull-arena": "^2.6.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-mung": "^0.5.1",
    "express-session": "^1.16.1",
    "helmet": "^3.18.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "swagger-ui-express": "^4.0.2"
  }
}
